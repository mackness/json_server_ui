{
    "name": "json-server-ui",
    "productName": "Json Server UI",
    "version": "1.0.0-beta.0",
    "description": "An Electron based UI for the awesome JSON Server project",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "start:client": "webpack --watch",
        "build:client": "webpack --production",
        "release": "npm run build:client && electron-packager . --ignore=\".git(ignore|modules)\" --out ./package",
        "prettier-watch": "onchange '**/*.js' -- prettier --write {{changed}}",
        "prettier": "prettier --write ./src/**/*.{scss,json,js,ts,tsx}",
        "build:server": "tsc"
    },
    "repository": "https://github.com/electron/electron-quick-start",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
