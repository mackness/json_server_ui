        "@babel/preset-react": "7.0.0",
        "@babel/preset-typescript": "7.3.3",
        "@types/app-root-dir": "0.1.0",
        "@types/express-handlebars": "0.0.30",
        "@types/http-errors": "1.6.1",
        "@types/json-server": "0.14.0",
        "@types/morgan": "1.7.35",
        "@types/prismjs": "1.9.1",
        "@types/react": "16.8.4",
